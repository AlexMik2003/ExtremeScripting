<!-- ListPrecedence Object XML Definition Start -->
  <xsd:complexType name="ListPrecedence">
    <xsd:annotation>
      <xsd:documentation>
        Information about the precedence of List Types
      ; Version : EXOS 15.1.2, Access : READ, WRITE</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="common:ExosBase">
        <xsd:sequence>
       <xsd:element name="listname1" type="ListNames" minOccurs="1">
          <xsd:annotation>
              <xsd:documentation> 
                 List type which has precedence over all list types. This list's entries are searched first to find role.
              ; Version : EXOS 15.1.2, Access : READ, WRITE</xsd:documentation>
          </xsd:annotation>
      </xsd:element>
       <xsd:element name="listname2" type="ListNames" minOccurs="1">
          <xsd:annotation>
              <xsd:documentation> 
                 List type which has next highest precedence, after listname1. This list's entries are searched after listname1 to find role.
              ; Version : EXOS 15.1.2, Access : READ, WRITE</xsd:documentation>
          </xsd:annotation>
      </xsd:element>
       <xsd:element name="listname3" type="ListNames" minOccurs="1">
          <xsd:annotation>
              <xsd:documentation> 
                 Specify list type which has least precedence of all. This list's entries are searched after all other lists to find role.
              ; Version : EXOS 15.1.2, Access : READ, WRITE</xsd:documentation>
          </xsd:annotation>
       </xsd:element>
        <xsd:element name="action" type="actionTypes" minOccurs="1">
            <xsd:annotation>
                  <xsd:documentation>action - configure/unconfigure ; Version : EXOS 15.1.2, Access : READ, WRITE</xsd:documentation>
             </xsd:annotation>
      </xsd:element>
       <xsd:element name="extension" type="common:ExtensionType" minOccurs="0"/>
          <xsd:any namespace="##other" processContents="lax" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute ref="common:operation"/>
        <xsd:anyAttribute namespace="##any" processContents="lax"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
