<!-- LDAPServerConfig Object XML Definition Start -->
  <xsd:complexType name="LdapServerConfig">
    <xsd:annotation>
      <xsd:documentation>
        information about the LDAP Servers COnfigured
      ; Version : EXOS 12.5, Access : READ, WRITE, CREATE, DELETE</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="common:ExosBase">
        <xsd:sequence>

          <xsd:element name="domain" type="common:StringLen64" minOccurs="1">
            <xsd:annotation>
              <xsd:documentation>Domain name; Version : EXOS 15.2, Access : INDEX, READ, WRITE</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="serverNo" type="common:uint16_t" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>sequence no. of the ldap server; Version : EXOS 12.5, Access : READ, INDEX</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="serverIp" type="common:IPAddress" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>ip address of the ldap server; Version : EXOS 12.5, Access : READ, WRITE, INDEX, CREATE, DELETE</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="serverHostName" type="common:StringLen256" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>host name of the ldap server; Version : EXOS 12.5, Access : READ, WRITE, INDEX, CREATE, DELETE</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="serverPort" type="common:uint16_t" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>port over which ldap client will talk to directory server; Version : EXOS 12.5, Access : READ, WRITE, INDEX</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="clientIp" type="common:IPAddress" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>ip address of ldap client; Version : EXOS 12.5, Access : READ, WRITE, INDEX</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="vrName" type="common:StringLen32" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>name of virtual router; Version : EXOS 12.5, Access : READ, WRITE, INDEX</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="securityMechanism" type="securityMechanismTypes" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Security Mechanism of the LDAP connection to this server; Version : EXOS 12.5, Access : READ, WRITE, INDEX</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="connStatus" type="connStatusTypes" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>connection status of server; Version : EXOS 12.5, Access : READ</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="requestsStat" type="common:uint32_t" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>requests sent to server from switch; Version : EXOS 12.5, Access : READ</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="responsesStat" type="common:uint32_t" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>responses sent to switch from server; Version : EXOS 12.5, Access : READ</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="errorsStat" type="common:uint32_t" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>error Statistics; Version : EXOS 12.5, Access : READ</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="extension" type="common:ExtensionType" minOccurs="0"/>
          <xsd:any namespace="##other" processContents="lax" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute ref="common:operation"/>
        <xsd:anyAttribute namespace="##any" processContents="lax"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
