              <xsd:documentation>Role Name given by user; Version : EXOS 12.5, Access : READ, INDEX, WRITE, CREATE, DELETE</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="matchCritera" type="common:StringLen1K" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Match Criteria to map this role to a detected user in Network; Version : EXOS 12.5, Access : READ, WRITE</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="isDefaultRole" type="common:TrueFalse" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Type of role. If true, the role is default; Version : EXOS 12.5, Access : READ</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="priority" type="common:uint8_t" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>priority assigned to the role; Version : EXOS 12.5, Access : READ, WRITE</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          
          <xsd:element name="vlanTag" type="common:uint32_t" minOccurs="1" maxOccurs="4094">
            <xsd:annotation>
              <xsd:documentation>VLAN Tag assigned to the role; Version : EXOS 15.4, Access : READ, WRITE</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          
          <xsd:element name="vrName" type="common:StringLen32" minOccurs="0">
            <xsd:annotation>
