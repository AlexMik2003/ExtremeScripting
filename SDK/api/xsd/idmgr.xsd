            <xsd:annotation>
              <xsd:documentation>name of child roles; Version : EXOS 12.5, Access : READ, WRITE, DELETE</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="noOfIdentities" type="common:uint16_t" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>number of identities assigned to the role; Version : EXOS 12.5, Access : READ</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="identityInfo" type="IdMgrIdentityInfo" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Related Identities to this role. ; Version : EXOS 12.5, Access : READ</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="action" type="actionTypes" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
                action - create/configure/unconfigure
              ; Version : EXOS 12.4, Access : READ, WRITE</xsd:documentation>
            </xsd:annotation>
          </xsd:element>

          <xsd:element name="extension" type="common:ExtensionType" minOccurs="0"/>
          <xsd:any namespace="##other" processContents="lax" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute ref="common:operation"/>
        <xsd:anyAttribute namespace="##any" processContents="lax"/>
