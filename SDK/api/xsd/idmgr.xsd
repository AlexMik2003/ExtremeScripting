<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.extremenetworks.com/XMLSchema/xos/idmgr" xmlns:common="http://www.extremenetworks.com/XMLSchema/xos/common" xmlns:idmgr="http://www.extremenetworks.com/XMLSchema/xos/idmgr" targetNamespace="http://www.extremenetworks.com/XMLSchema/xos/idmgr" version="A.B.C.D">
    <xsd:import namespace="http://www.extremenetworks.com/XMLSchema/xos/common" schemaLocation="./common.xsd"/>
    <xsd:simpleType name="LoginStatusTypes">
        <xsd:annotation>
        	<xsd:documentation>Enumeration of login status.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="unknown"/>
            <xsd:enumeration value="loggedOn"/>
            <xsd:enumeration value="loggedOut"/>
            <xsd:enumeration value="na"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="AuthMethodTypes">
        <xsd:annotation>
        	<xsd:documentation>Enumeration of NetLogin authentication method.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="unknown"/>
            <xsd:enumeration value="netloginDot1x"/>
            <xsd:enumeration value="netloginWeb"/>
            <xsd:enumeration value="netloginMac"/>
            <xsd:enumeration value="kerberos"/>
            <xsd:enumeration value="na"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="FeatureStatusTypes">
        <xsd:annotation>
        	<xsd:documentation>Enumeration of feature status.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="unknown"/>
            <xsd:enumeration value="enabled"/>
            <xsd:enumeration value="disabled"/>
            <xsd:enumeration value="na"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="IdentityManagerEventTypes">
        <xsd:annotation>
        	<xsd:documentation>Enumeration of identity management event types </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="unknown"/>
            <xsd:enumeration value="discover"/>
            <xsd:enumeration value="undiscover"/>
            <xsd:enumeration value="reauth"/>
            <xsd:enumeration value="authFailure"/>
            <xsd:enumeration value="ipAddrAdd"/>
            <xsd:enumeration value="ipAddrDelete"/>
            <xsd:enumeration value="securityViolation"/>
            <xsd:enumeration value="identityInfoUpdate"/>
            <xsd:enumeration value="identityRoleUpdate"/>
            <xsd:enumeration value="na"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="AddressTypes">
        <xsd:annotation>
        	<xsd:documentation>Enumeration of Address types supported by Identity Manager.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="unknown"/>
            <xsd:enumeration value="mac"/>
            <xsd:enumeration value="ip"/>
            <xsd:enumeration value="na"/>
        </xsd:restriction>
    </xsd:simpleType>


    <xsd:simpleType name="DatasetTypes">
        <xsd:annotation>
        	<xsd:documentation>Enumeration of identity information data set type.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="unknown"/>
            <xsd:enumeration value="active"/>
            <xsd:enumeration value="event"/>
            <xsd:enumeration value="na"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="actionTypes">
        <xsd:annotation>
        	<xsd:documentation>Enumeration of feature status.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="unknown"/>
            <xsd:enumeration value="create"/>
            <xsd:enumeration value="configure"/>
            <xsd:enumeration value="unconfigure"/>
            <xsd:enumeration value="delete"/>
            <xsd:enumeration value="na"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="connStatusTypes">
        <xsd:annotation>
        	<xsd:documentation>Enumeration of connection status types.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="unknown"/>
            <xsd:enumeration value="notActive"/>
            <xsd:enumeration value="Active"/>
            <xsd:enumeration value="ipAddressUnresolved"/>
            <xsd:enumeration value="na"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="securityMechanismTypes">
        <xsd:annotation>
        	<xsd:documentation>Enumeration of security mechanism types.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="unknown"/>
            <xsd:enumeration value="plainText"/>
            <xsd:enumeration value="SASLDigestMD5"/>
            <xsd:enumeration value="SSL"/>
            <xsd:enumeration value="na"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="ListTypes">
        <xsd:annotation>
        	<xsd:documentation>Enumeration of List entry types supported by Identity Manager.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="greylist"/>
            <xsd:enumeration value="blacklist"/>
            <xsd:enumeration value="whitelist"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="ListNames">
        <xsd:annotation>
        	<xsd:documentation>Enumeration of List names supported by Identity Manager.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="greylist"/>
            <xsd:enumeration value="blacklist"/>
            <xsd:enumeration value="whitelist"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="ListEntryTypes">
        <xsd:annotation>
        	<xsd:documentation>Enumeration of Blacklist/Whitelist entry types supported by Identity Manager.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="unknown"/>
            <xsd:enumeration value="mac"/>
            <xsd:enumeration value="ip"/>
            <xsd:enumeration value="username"/>
            <xsd:enumeration value="na"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="GreyListEntryTypes">
        <xsd:annotation>
        	<xsd:documentation>Enumeration of Greylist entry types supported by Identity Manager.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="unknown"/>
            <xsd:enumeration value="username"/>
            <xsd:enumeration value="na"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- User Identity Data Object XML Definition Start -->
    <xsd:complexType name="UserIdentityData">
        <xsd:annotation>
            <xsd:documentation>
                User identity information. Identity information for an active user or an
                identity management event.
                <br/>
                Note: This object is used both for quering active identity data as well as event data. The datasetType field is the mandatory filtering parameter to be set. For querying active user data, datasetType field should be set to "active" and for querying event data this field should be set to "event".
            ; Version : EXOS 12.4, Access : READ, INDEX </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="common:ExosBase">
                <xsd:sequence>
                    <xsd:element name="datasetType" type="DatasetTypes" minOccurs="1" >
                        <xsd:annotation>
                            <xsd:documentation>Data set type. Data set type from which information is populated in this object. For querying active user data this field should be set to "active" and for querying the event data this field should be set to "event". This field is a mandatory field for doing query. For "event" dataset type, only modification timestamp based query is supported. userName, domain and portList can not be specified in the queries for "event" dataset.
                            ; Version : EXOS 12.4, Access : READ, INDEX</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="userName" type="common:StringLen64" minOccurs="0" >
                        <xsd:annotation>
                            <xsd:documentation>Name of the user. The active data can be queried for a given userName and domain combination. For protocols which do not use domain (i.e., Netlogin MAC), only userName can be used to query the data for given user. This field can be specified only with domain and datasetType field for queries.; Version : EXOS 12.4, Access : READ, INDEX</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="domain" type="common:StringLen64" minOccurs="0" >
                        <xsd:annotation>
                            <xsd:documentation>Domain name. Kerberos realm or domain name provided by Dot1x. The active data can be queried for a given user and domain name combination. For protocols which do not use domain name (i.e., Netlogin MAC), this field can be left empty. This field can only be speficied with userName field and  datasetType fields. Query based only on domain is not supported.
                            ; Version : EXOS 12.4, Access : READ, INDEX</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="roleName" type="common:StringLen32" minOccurs="0" >
                        <xsd:annotation>
                            <xsd:documentation>Role name. The user's role as determined by Identity Manager based on the user's attributes. ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="portList" type="common:StringLen16" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>List of ports on which user is logged in. This field can be sent as part of query to get the user identity information about identities only on the given portlist. This field can be specified only with datasetType field for queries.
                            ; Version : EXOS 12.4, Access : READ, INDEX</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="modificationTimestamp" type="common:int64_t" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation> Timestamp of last modification of record. This is the number of milliseconds since the epoch. <br/> This field can be sent in the query as to get the information about the identities modified or events generated after the given time and the query time. This field can be specified only with datasetType field for queries.; Version : EXOS 12.4, Access : READ,INDEX</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="eventType" type="IdentityManagerEventTypes" minOccurs="0" >
                        <xsd:annotation>
                            <xsd:documentation> Identity management event type. For
                            active user identity data, this field
                            is set to "na". For identity events, it is set to
                            appropriate event type.
                            ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="authProtocolsUsed" type="common:StringLen64" minOccurs="0" >
                        <xsd:annotation>
                            <xsd:documentation>List of authentication protocols used by the user for login.
                            ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="numOfLocations" type="common:uint32_t" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation> Number of locations on which user is currently logged on.
                            ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="creationTimestamp" type="common:int64_t" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation>Record creation timestamp. This is the number of milliseconds since the epoch.; Version : EXOS 12.4, Access : READ</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="location" type="Location" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>This object represents detailed information about a login instance including location information i.e. port instance. ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="extension" type="common:ExtensionType" minOccurs="0"/>
                    <xsd:any namespace="##other" processContents="lax" minOccurs="0"/>
                </xsd:sequence>
                <xsd:attribute ref="common:operation"/>
                <xsd:anyAttribute namespace="##any" processContents="lax"/>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <!-- User Identity Data Object XML Definition End -->

    <!-- Location Object XML Definition Start -->
    <xsd:complexType name="Location">
        <xsd:annotation>
            <xsd:documentation>Detailed information about the login instance including port
                and MAC address information.
            ; Version : EXOS 12.4, Access : READ</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="port" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Port Instance used for login; Version : EXOS 12.4, Access : READ, INDEX</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="portDisplayString" type="common:StringLen32" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation> Display string associated with Port.
                    ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="macAddress" type="common:StringLen32" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>MAC Address used for login.; Version : EXOS 12.4, Access : READ, INDEX </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="roleName" type="common:StringLen32" minOccurs="0" >
                <xsd:annotation>
                        <xsd:documentation>The user's role in this location as determined by Identity Manager based on the user's attributes. ; Version : EXOS 12.7, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="lldpCapabilityInfo" type="common:uint32_t" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Capability information for LLDP device(s).  This is a bitmap and more than one bits can be set. Following are the details:
                            bit 0: OTHER,
                            bit 1: REPEATER,
                            bit 2: BRIDGE,
                            bit 3: WLAN_ACCESS_PT,
                            bit 4: ROUTER,
                            bit 5: GEN_TEL_PHONE,
                            bit 6: DOCSIS_CABLE_SER,
                            bit 7: STATION_ONLY,
                            bit 8-31: RSVD,
                    ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="lldpModelName" type="common:StringLen32" minOccurs="0" >
                <xsd:annotation>
                        <xsd:documentation>LLDP Model Name : The Model/Type of the LLDP device detected at this location.; Version : EXOS 12.7, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="lldpManufacturer" type="common:StringLen32" minOccurs="0" >
                <xsd:annotation>
                        <xsd:documentation>LLDP device Manufacturer : The vendor name of the LLDP device detected at this location. ; Version : EXOS 12.7, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="lldpSysDescription" type="common:StringLen256" minOccurs="0" >
                <xsd:annotation>
                        <xsd:documentation>LLDP device System Description : System description string advertised by the LLDP device detected at this location. ; Version : EXOS 15.1, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="netBiosHostName" type="common:StringLen32" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Netbios hostname for Kerberos authenticated users. All
                    login instances may not have this information.
                    ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="kerberosSnooping" type="common:TrueFalse" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Flag to indicate Kerberos authenticated
                    login instance. ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="authMethod" type="idmgr:AuthMethodTypes" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>NetLogin authentication method.; Version : EXOS 12.4, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="securityProfile" type="common:uint32_t" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Bitmap representing security features enabled on the
                    port. Following are the details:
                            bit 0: ARP validation,
                            bit 1: DoS protection,
                            bit 2: Gratuitous ARP protection,
                            bit 3: Source IP lock down,
                            bit 4: DHCP snooping,
                            bit 5-bit31: Reserved for future usage
                     ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="securityViolations" type="common:uint32_t" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation> Bitmap representing security violations detected on
                    port used by user to logon. Following are the details:
                        bit 0: ARP validation violation,
                        bit 1: DoS violation,
                        bit 2: Gratuitous ARP violation,
                        bit 3: Source IP locked down,
                        bit 4: Rogue DHCP server detected,
                        bit 5-bit31: Reserved for future usage

                    ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="logonStatus" type="idmgr:LoginStatusTypes" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Current login status of the user on this
                    location.
                    ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="logonTime" type="common:int64_t" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Timestamp of logon on this location. This is the number of milliseconds since the epoch.; Version : EXOS 12.4, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="logOutTime" type="common:int64_t" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Timestamp of logout on this location. This is the number of milliseconds since the epoch. If the user is not logged out the value is set to zero.
                    ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="authFailTime" type="common:int64_t" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation> Timestamp of unsuccessful authentication. This is the number of milliseconds since the epoch. If the authentication is successful, value is set to zero.
                    ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="vlanInfo" type="IdMgrVlanInfo" minOccurs="0">
                <xsd:annotation>
                     <xsd:documentation>VLANs and associated IP address(es) detail. ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="modificationTimestamp" type="common:int64_t" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Timestamp of record modification. This is the number of milliseconds since the epoch.
                    ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            
            <xsd:element name="extension" type="common:ExtensionType" minOccurs="0"/>
            <xsd:any namespace="##other" processContents="lax" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute ref="common:operation"/>
        <xsd:anyAttribute namespace="##any" processContents="lax"/>
    </xsd:complexType>
    <!-- Location Object XML Definition End -->

    <!-- IdMgrVlan Object XML Definition Start -->
    <xsd:complexType name="IdMgrVlan">
        <xsd:annotation>
            <xsd:documentation>VLAN information. This object is used to keep the VLAN and associated IP address information.; Version : EXOS 12.4, Access : READ</xsd:documentation>
        </xsd:annotation>
            <xsd:sequence>
            <xsd:element name="name" type="common:StringLen32" minOccurs="0">
                <xsd:annotation>
                        <xsd:documentation> Vlan Name.; Version : EXOS 12.4, Access : READ</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ipAddress" type="common:IPAddress" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                        <xsd:documentation>IP address used; Version : EXOS 12.4, Access : READ</xsd:documentation>
                    </xsd:annotation>
            </xsd:element>
            <xsd:element name="extension" type="common:ExtensionType" minOccurs="0"/>
            <xsd:any namespace="##other" processContents="lax" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute ref="common:operation"/>
            <xsd:anyAttribute namespace="##any" processContents="lax"/>
    </xsd:complexType>
    <!-- IdMgrVlan Object XML Definition End -->

    <!-- IdMgrVlanInfo Object XML Definition Start -->
    <xsd:complexType name="IdMgrVlanInfo">
        <xsd:annotation>
            <xsd:documentation>VLAN information. This object is used to keep information about all the VLAN and associated IP address information.; Version : EXOS 12.4, Access : READ</xsd:documentation>
        </xsd:annotation>
            <xsd:sequence>
                <xsd:element name="vlan" type="IdMgrVlan" minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                            <xsd:documentation> Vlan ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
            	<xsd:element name="extension" type="common:ExtensionType" minOccurs="0"/>
            	<xsd:any namespace="##other" processContents="lax" minOccurs="0"/>
            </xsd:sequence>
        <xsd:attribute ref="common:operation"/>
        <xsd:anyAttribute namespace="##any" processContents="lax"/>
    </xsd:complexType>
    <!-- IdMgrVlanInfo Object XML Definition End -->



    <!-- IdentityManagerConfig Object XML Definition Start -->
    <xsd:complexType name="IdentityManagerConfig">
        <xsd:annotation>
            <xsd:documentation>
            Represents configuration information for identity management feature. All configuration parameter for identity management feature can be accessed or set using this object.
            ; Version : EXOS 12.4, Access : READ, WRITE</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="common:ExosBase">
                <xsd:sequence>
                    <xsd:element name="featureStatus" type="FeatureStatusTypes" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation> Identity management feature status.
                            ; Version : EXOS 12.4, Access : READ, WRITE</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="maxIdDatabaseSize" type="common:uint16_t" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation> Maximum identity database size in kilo bytes. The valid range is from 64 kilobytes to 49152 kilobytes. The default value is 512 kilobytes.
                            ; Version : EXOS 12.4, Access : READ, WRITE</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="expiredEntryTimeout" type="common:uint16_t" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation> Entry expiry age out time value in seconds. The valid range is from 60 seconds to 1800 seconds. By default the value is set to 180 seconds.
                            ; Version : EXOS 12.4, Access : READ, WRITE</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="effectiveExpiredEntryTimeout" type="common:uint16_t" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation> Internally adjusted entries expiry age out time.
                            Expired entry timeout  is internally adjusted  for
                            effective memory management.
                            ; Version : EXOS 12.4, Access : READ</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="idMgmtEnabledPorts" type="common:StringLen32" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation> List of ports on which identity management is enabled or required to be enabled (for set operation).
                            ; Version : EXOS 12.4, Access : READ, WRITE</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="fdbDetectionStatus" type="FeatureStatusTypes" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation> Identity management FDB detection status.
                            ; Version : EXOS 15.2, Access : READ, WRITE</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="idMgmtFdbDisabledPorts" type="common:StringLen32" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation> List of ports on which identity management FDB detection is disabled or required to be enabled (for set operation).
                            ; Version : EXOS 15.2, Access : READ, WRITE</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="iparpDetectionStatus" type="FeatureStatusTypes" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation> Identity management IPARP detection status.
                            ; Version : EXOS 15.2, Access : READ, WRITE</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="idMgmtIpArpDisabledPorts" type="common:StringLen32" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation> List of ports on which identity management IPARP detection is disabled or required to be enabled (for set operation).
                            ; Version : EXOS 15.2, Access : READ, WRITE</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="ipSecurityDetectionStatus" type="FeatureStatusTypes" minOccurs="0">
                        <xsd:annotation>
                            <xsd:documentation> Identity management IP Security detection status.
                            ; Version : EXOS 15.2, Access : READ, WRITE</xsd:documentation>
