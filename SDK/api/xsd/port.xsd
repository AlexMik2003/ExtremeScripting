	<xsd:element name="zzz" type="objectList"/>
	<xsd:complexType name="objectList">
		<xsd:annotation>
			<xsd:documentation>
			(Internal Use)
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="moduleObject" type="AccessObject" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
