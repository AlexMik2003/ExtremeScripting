<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.extremenetworks.com/XMLSchema/xos/port" xmlns:common="http://www.extremenetworks.com/XMLSchema/xos/common" xmlns:port="http://www.extremenetworks.com/XMLSchema/xos/port" targetNamespace="http://www.extremenetworks.com/XMLSchema/xos/port" version="A.B.C.D">
	<xsd:import namespace="http://www.extremenetworks.com/XMLSchema/xos/common" schemaLocation="./common.xsd"/>



  <xsd:simpleType name="PortTransceiverStatus">
         <xsd:restriction base="xsd:string">
                <xsd:enumeration value="N/A"/>
                <xsd:enumeration value="Normal"/>
                <xsd:enumeration value="Low Alarm"/>
                <xsd:enumeration value="High Alarm"/>
                <xsd:enumeration value="Low Warning"/>
                <xsd:enumeration value="High Warning"/>
         </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="PortTransceiverInfo">
          <xsd:annotation>
                  <xsd:documentation>
                        DDMI is a Digital Diagnostic Monitoring feature on optics modules.It helps check if the module is operating within the manufacturer prescribed operating conditions.
                  ; Version : EXOS 12.5, Access : </xsd:documentation>
          </xsd:annotation>
       <xsd:complexContent>
            <!-- 
            * All external objects extend the ExosBase object, which is just 
            * a place holder for the content that you specify in this file.
            -->
          <xsd:extension base="common:ExosBase">
                <xsd:sequence>
                      <xsd:element name="portList" type="common:StringLen8" minOccurs="0">
                            <xsd:annotation>
                                  <xsd:documentation>
                                    Specifies the port number(s).
                                  ; Version : EXOS 12.5, Access : READ, INDEX</xsd:documentation>
                            </xsd:annotation>
                     </xsd:element>
           
                      <xsd:element name="mediaType" type="common:StringLen32" minOccurs="0">
                          <xsd:annotation>
                                <xsd:documentation>
                                  Shows Media type of tranceiver.
                                ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                         </xsd:annotation>
                      </xsd:element>

                    <xsd:element name="partNumber" type="common:StringLen32" minOccurs="0">
                          <xsd:annotation>
                                <xsd:documentation>
                                 Part number of transceiver.
                                ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                         </xsd:annotation>
                    </xsd:element>

                    <xsd:element name="serialNumber" type="common:StringLen32" minOccurs="0">
                          <xsd:annotation>
                                 <xsd:documentation>
                                   Serial number of transceiver.
                                 ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                          </xsd:annotation> 
                    </xsd:element>
                                 
                    <xsd:element name="temperature" type="xsd:float" minOccurs="0">
                          <xsd:annotation>
                                <xsd:documentation>
                                  Shows internally measured transceiver temperature in Celsius. 
                               ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                         </xsd:annotation> 
                     </xsd:element>

 
                     <xsd:element name="lowWarnTshldTemp" type="xsd:float" minOccurs="0">
                               <xsd:annotation>
                                      <xsd:documentation>
                                       Shows internally measured low warning threshold transceiver temperature in Celsius. 
                                      ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                                </xsd:annotation> 
                     </xsd:element>
                   
                      <xsd:element name="highWarnTshldTemp" type="xsd:float" minOccurs="0">
                            <xsd:annotation>
                                  <xsd:documentation>
                                       Shows internally measured high warning threshold transceiver temperature in Celsius. 
                                  ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                            </xsd:annotation> 
                      </xsd:element>
                   
                      <xsd:element name="lowAlarmTshldTemp" type="xsd:float" minOccurs="0">
                             <xsd:annotation>
                                   <xsd:documentation>
                                        Shows internally measured low alarm threshold transceiver temperature in Celsius. 
                                   ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                             </xsd:annotation> 
                      </xsd:element>
                    
                      <xsd:element name="highAlarmTshldTemp" type="xsd:float" minOccurs="0">
                           <xsd:annotation>
                                 <xsd:documentation>
                                      Shows internally measured high alarm threshold transceiver temperature in Celsius. 
                                 ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                          </xsd:annotation> 
                      </xsd:element>

                      <xsd:element name="temperatureStatus" type="PortTransceiverStatus" minOccurs="0">
                              <xsd:annotation>
                                      <xsd:documentation>
                                      Shows temperature status of transceiver. 
                                     ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                              </xsd:annotation> 
                      </xsd:element>

                      <xsd:element name="txPower" type="xsd:float" minOccurs="0">
                              <xsd:annotation>
                                      <xsd:documentation>
                                        Shows TX output power in dBm. 
                                      ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                              </xsd:annotation> 
                      </xsd:element>

 
                      <xsd:element name="lowWarnTshldTxPower" type="xsd:float" minOccurs="0">
                              <xsd:annotation>
                                      <xsd:documentation>
                                       Shows low warning threshold TX output power in dBm. 
                                      ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                               </xsd:annotation> 
                       </xsd:element>
                     
                    
                       <xsd:element name="highWarnTshldTxPower" type="xsd:float" minOccurs="0">
                               <xsd:annotation>
                                     <xsd:documentation>
                                         Shows high warning threshold TX output power in dBm. 
                                     ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                                </xsd:annotation> 
                       </xsd:element>
                   
                       <xsd:element name="lowAlarmTshldTxPower" type="xsd:float" minOccurs="0">
                               <xsd:annotation>
                                      <xsd:documentation>
                                       Shows low alarm threshold TX output power in dBm. 
                                       ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                               </xsd:annotation> 
                        </xsd:element>
                    
                       <xsd:element name="highAlarmTshldTxPower" type="xsd:float" minOccurs="0">
                                 <xsd:annotation>
                                           <xsd:documentation>
                                           Shows high alarm threshold TX output power in dBm.
                                           ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                                  </xsd:annotation> 
                      </xsd:element>

                      <xsd:element name="txPowerStatus" type="PortTransceiverStatus" minOccurs="0">
                              <xsd:annotation>
                                      <xsd:documentation>
                                          Shows TX output power status.  
                                      ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                              </xsd:annotation> 
                      </xsd:element>

                      <xsd:element name="rxPower" type="xsd:float" minOccurs="0">
                             <xsd:annotation>
                                     <xsd:documentation>
                                        Shows RX input power in dBm. 
                                    ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                             </xsd:annotation> 
                       </xsd:element>

                      <xsd:element name="highWarnTshldRxPower" type="xsd:float" minOccurs="0">
                             <xsd:annotation>
                               <xsd:documentation>
                                    Shows high warning threshold RX input power in dBm. 
                                 ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                           </xsd:annotation> 
                     </xsd:element>
                   
                      <xsd:element name="lowWarnTshldRxPower" type="xsd:float" minOccurs="0">
                              <xsd:annotation>
                                   <xsd:documentation>
                                     Shows low warning threshold RX input power in dBm. 
                                   ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                               </xsd:annotation> 
                      </xsd:element>
                   
                    
             
                     <xsd:element name="lowAlarmTshldRxPower" type="xsd:float" minOccurs="0">
                         <xsd:annotation>
                               <xsd:documentation>
                                Shows low alarm threshold RX input power in dBm. 
                               ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                        </xsd:annotation> 
                    </xsd:element>
                    
                    <xsd:element name="highAlarmTshldRxPower" type="xsd:float" minOccurs="0">
                             <xsd:annotation>
                                    <xsd:documentation>
                                     Shows high alarm threshold RX input power in dBm.
                                    ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                            </xsd:annotation> 
                   </xsd:element>

                 <xsd:element name="rxPowerStatus" type="PortTransceiverStatus" minOccurs="0">
                         <xsd:annotation>
                                <xsd:documentation>
                               Shows RX input power status.
                               ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                        </xsd:annotation> 
                 </xsd:element>
                                   
                 <xsd:element name="txBiasCurrent" type="xsd:float" minOccurs="0">
                     <xsd:annotation>
                            <xsd:documentation>
                            Shows Tx bias current in milliampere.  
                            ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                     </xsd:annotation> 
                </xsd:element>

 
                <xsd:element name="lowWarnTshldTxBiasCurrent" type="xsd:float" minOccurs="0">
                      <xsd:annotation>
                             <xsd:documentation>
                             Shows low warning threshold Tx bias current in milliampere.  
                             ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                     </xsd:annotation> 
               </xsd:element>
                     
                    
               <xsd:element name="highWarnTshldTxBiasCurrent" type="xsd:float" minOccurs="0">
                     <xsd:annotation>
                           <xsd:documentation>
                            Shows high warning threshold Tx bias current in milliampere.  
                           ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                    </xsd:annotation> 
               </xsd:element>
                   
               <xsd:element name="lowAlarmTshldTxBiasCurrent" type="xsd:float" minOccurs="0">
                      <xsd:annotation>
                              <xsd:documentation>
                             Shows low alarm threshold Tx bias current in milliampere.  
                             ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                      </xsd:annotation> 
               </xsd:element>
                    
               <xsd:element name="highAlarmTshldTxBiasCurrent" type="xsd:float" minOccurs="0">
                       <xsd:annotation>
                             <xsd:documentation>
                             Shows high alarm threshold Tx bias current in milliampere.  
                             ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                      </xsd:annotation> 
               </xsd:element>

              <xsd:element name="txBiasCurrentStatus" type="PortTransceiverStatus" minOccurs="0">
                      <xsd:annotation>
                              <xsd:documentation>
                               Shows Tx bias current status.
                              ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                      </xsd:annotation> 
              </xsd:element>


              <xsd:element name="Aux1Voltage" type="xsd:float" minOccurs="0">
                     <xsd:annotation>
                            <xsd:documentation>
                            Shows input supply voltages to the transceiver. Some transceivers monitor one voltage, some transceivers monitor two voltages.
                            ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                    </xsd:annotation> 
             </xsd:element>

 
             <xsd:element name="lowWarnTshldAux1Voltage" type="xsd:float" minOccurs="0">
                     <xsd:annotation>
                          <xsd:documentation>
                           Shows low warning threshold Aux-1 input supply voltage in Volts.  
                           ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                    </xsd:annotation> 
             </xsd:element>
                     
            <xsd:element name="highWarnTshldAux1Voltage" type="xsd:float" minOccurs="0">
                    <xsd:annotation>
                          <xsd:documentation>
                            Shows high warning threshold Aux-1 input supply voltage in Volts.  
                           ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                   </xsd:annotation> 
            </xsd:element>
                   
            <xsd:element name="lowAlarmTshldAux1Voltage" type="xsd:float" minOccurs="0">
                    <xsd:annotation>
                          <xsd:documentation>
                          Shows low alarm threshold Aux-1 input supply voltage in Volts.
                          ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                   </xsd:annotation> 
           </xsd:element>
                    
           <xsd:element name="highAlarmTshldAux1Voltage" type="xsd:float" minOccurs="0">
                   <xsd:annotation>
                          <xsd:documentation>
                          Shows high alarm threshold Aux-1 input supply voltage in Volts.  
                          ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                 </xsd:annotation> 
           </xsd:element>

           <xsd:element name="aux1VoltageStatus" type="PortTransceiverStatus" minOccurs="0">
                    <xsd:annotation>
                            <xsd:documentation>
                            Shows status of Aux-1 input supply voltages to the transceiver. 
                            ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                   </xsd:annotation> 
          </xsd:element>


          <xsd:element name="Aux2Voltage" type="xsd:float" minOccurs="0">
                   <xsd:annotation>
                            <xsd:documentation>
                            Shows input supply voltages to the transceiver. Some transceivers monitor two voltages.
                            ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                   </xsd:annotation> 
          </xsd:element>

 
          <xsd:element name="lowWarnTshldAux2Voltage" type="xsd:float" minOccurs="0">
                  <xsd:annotation>
                          <xsd:documentation>
                           Shows low warning threshold Aux2 input supply voltage in Volts.  
                          ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                 </xsd:annotation> 
          </xsd:element>
                     
                    
           <xsd:element name="highWarnTshldAux2Voltage" type="xsd:float" minOccurs="0">
                  <xsd:annotation>
                        <xsd:documentation>
                         Shows high warning threshold Aux2 input supply voltage in Volts.  
                        ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                  </xsd:annotation> 
          </xsd:element>
                   
           <xsd:element name="lowAlarmTshldAux2Voltage" type="xsd:float" minOccurs="0">
                   <xsd:annotation>
                            <xsd:documentation>
                           Shows low alarm threshold Aux2 input supply voltage in Volts.  
                            ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                  </xsd:annotation> 
           </xsd:element>
                    
           <xsd:element name="highAlarmTshldAux2Voltage" type="xsd:float" minOccurs="0">
                   <xsd:annotation>
                        <xsd:documentation>
                        Shows high alarm threshold Aux2 input supply voltage in Volts.  
                          ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                  </xsd:annotation> 
          </xsd:element>
          <xsd:element name="aux2VoltageStatus" type="PortTransceiverStatus" minOccurs="0">
                  <xsd:annotation>
                          <xsd:documentation>
                           Shows status of Aux-2 input supply voltage to the transceiver.
                          ; Version : EXOS 12.5, Access : READ</xsd:documentation>
                 </xsd:annotation> 
          </xsd:element>

                        
          <xsd:element name="extension" type="common:ExtensionType" minOccurs="0"/>
          <xsd:any namespace="##other" processContents="lax" minOccurs="0"/>
     </xsd:sequence>            
     <xsd:attribute ref="common:operation"/>
     <xsd:anyAttribute namespace="##any" processContents="lax"/>
     </xsd:extension>
     </xsd:complexContent>
  </xsd:complexType>

   
	<xsd:simpleType name="LinkStates">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="Ready"/>
			<xsd:enumeration value="Active"/>
			<xsd:enumeration value="NotPresent"/>
			<xsd:enumeration value="na"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Percent">
		<xsd:restriction base="xsd:float">
			<xsd:minExclusive value="0.0"/>
			<xsd:maxExclusive value="100.0"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="QoSTypes">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="ingress"/>
			<xsd:enumeration value="egress"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="QoSProfiles">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="none"/>
			<xsd:enumeration value="QP1"/>
			<xsd:enumeration value="QP2"/>
			<xsd:enumeration value="QP3"/>
			<xsd:enumeration value="QP4"/>
			<xsd:enumeration value="QP5"/>
			<xsd:enumeration value="QP6"/>
			<xsd:enumeration value="QP7"/>
			<xsd:enumeration value="QP8"/>
			<xsd:enumeration value="na"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="QoSProfileInfo">
		<xsd:annotation>
			<xsd:documentation>
			QoS profile information.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="qos" type="QoSProfiles" minOccurs="0"/>
			<xsd:element name="minBw" type="common:uint32_t" minOccurs="0"/>
			<xsd:element name="maxBw" type="common:uint32_t" minOccurs="0"/>
			<xsd:element name="priority" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:byte">
						<xsd:minExclusive value="0"/>
						<xsd:maxExclusive value="7"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AggrQoSProfileInfo">
		<xsd:annotation>
			<xsd:documentation>
			Aggregate QoS profile information.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="minBw" type="common:uint32_t" minOccurs="0"/>
			<xsd:element name="maxBw" type="common:uint32_t" minOccurs="0"/>
			<xsd:element name="priority" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:byte">
						<xsd:minExclusive value="0"/>
						<xsd:maxExclusive value="7"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="VlanInfo">
		<xsd:sequence>
			<xsd:element name="vlanName" type="common:StringLen64" minOccurs="0"/>
                </xsd:sequence>
        </xsd:complexType>
	
	<xsd:simpleType name="RateShaping">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="Unsupported"/>
			<xsd:enumeration value="SupportIQP1_2"/>
			<xsd:enumeration value="SupportIQP1_8"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AdminStates">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="Enabled"/>
			<xsd:enumeration value="Disabled"/>
			<xsd:enumeration value="na"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Protocols">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="UP"/>
			<xsd:enumeration value="na"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PreferredMediums">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="copper"/>
			<xsd:enumeration value="fiber"/>
			<xsd:enumeration value="na"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Speeds">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="10"/>
			<xsd:enumeration value="100"/>
			<xsd:enumeration value="1000_1G"/>
			<xsd:enumeration value="10000_10G"/>
			<xsd:enumeration value="AUTO"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Media">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="copper"/>
			<xsd:enumeration value="fiber"/>
			<xsd:enumeration value="na"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AutoNegotiations">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="on"/>
			<xsd:enumeration value="off"/>
			<xsd:enumeration value="na"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DuplexValues">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="half"/>
			<xsd:enumeration value="full"/>
			<xsd:enumeration value="auto"/>
			<xsd:enumeration value="na"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="EtherTypes">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="primary"/>
			<xsd:enumeration value="secondry"/>
			<xsd:enumeration value="na"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="FloodingTypes">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="unicast"/>
			<xsd:enumeration value="multicast"/>
			<xsd:enumeration value="unicast_multicast"/>
			<xsd:enumeration value="broadcast"/>
			<xsd:enumeration value="unicast_broadcast"/>
			<xsd:enumeration value="multicast_broadcast"/>
			<xsd:enumeration value="all"/>
			<xsd:enumeration value="none"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<!--  6 {show -f "address based - L2_L3_CHK_SUM\n"}
                            7 {show -f "address based - L2_L3_L4_CHK_SUM\n"}
                            8 {show -f "address based - L3\n"}
                            9 {show -f "address based - L3_L4\n"}
-->
	
		<xsd:simpleType name="SharingAlgoritms">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="unknown"/>
			<xsd:enumeration value="L2"/>
			<xsd:enumeration value="L3"/>
			<xsd:enumeration value="L4"/>
			<xsd:enumeration value="L2_L3"/>			
			<xsd:enumeration value="L3_L4"/>			
			<xsd:enumeration value="L2_L3_L4"/>
			<xsd:enumeration value="L2_L3_CHK_SUM"/>
			<xsd:enumeration value="L2_L3_L4_CHK_SUM"/>		
            <xsd:enumeration value="SRC_MAC"/>
            <xsd:enumeration value="DEST_MAC"/>
            <xsd:enumeration value="SRC_IP"/>
            <xsd:enumeration value="DEST_IP"/>
            <xsd:enumeration value="SRC_IPv6"/>
            <xsd:enumeration value="DEST_IPv6"/>
			<xsd:enumeration value="custom"/>
			<xsd:enumeration value="na"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="LoadSharing">
	     <xsd:annotation>
			<xsd:documentation>Load sharing group.; Version : EXOS 12.0, Access : READ,ADD, WRITE</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="common:ExosBase">
				<xsd:sequence>
					<xsd:element name="masterPort"  type="common:StringLen8"  minOccurs="0" >
						<xsd:annotation>
							<xsd:documentation>Master port for load sharing; Version : EXOS 12.0, Access : READ, INDEX</xsd:documentation>
						</xsd:annotation>
						</xsd:element>
					<xsd:element name="algorithm"  type="SharingAlgoritms"  minOccurs="0" >
						<xsd:annotation>
							<xsd:documentation>Algoritm for load sharing; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
						</xsd:element>
					<xsd:element name="memberPorts"  type="common:StringLen1K"  minOccurs="0" >
						<xsd:annotation>
							<xsd:documentation>Member ports participating in this sharing; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
						</xsd:element>
						
			
					<xsd:element name="extension" type="common:ExtensionType" minOccurs="0"/>
					<xsd:any namespace="##other" processContents="lax" minOccurs="0"/>
				</xsd:sequence>
				<xsd:attribute ref="common:operation"/>
				<xsd:anyAttribute namespace="##any" processContents="lax"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="FloodRate">
