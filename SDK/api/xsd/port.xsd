	<xsd:complexType name="PortTrafficStats">
		<xsd:annotation>
			<xsd:documentation>Real-time port statistics.; Version : EXOS 12.0, Access : READ</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="common:ExosBase">
				<xsd:sequence>
					<xsd:element name="portList" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>
							Port number. When referring to a single port as in a <code>getRequest</code> to fetch specific port, the value
							is just that port number.
							<br/>
							This is also used to specify a set of ports in a <code>getRequest</code> to fetch
							multiple ports. In this case, the value is a comma separated list of ports or port ranges.
							For example, "1:1, 1:3, 2:1-2:3" ports 1:1, 1:3, 2:1,2:2,2:3.
							; Version : EXOS 12.0, Access : READ, INDEX</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="linkState" type="LinkStates" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Port link state.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="displayString" type="common:StringLen16" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>User configured name or display string of the port.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transmitPacketCounter" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Packets transmitted.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transmitByteCounter" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Bytes transmitted.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="receivePacketCounter" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Packets received.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="receiveByteCounter" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Bytes received.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="receiveBCastCounter" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Broadcast packets received.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="receiveMCastCounter" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Multicast packets received.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="extension" type="common:ExtensionType" minOccurs="0"/>
					<xsd:any namespace="##other" processContents="lax" minOccurs="0"/>
				</xsd:sequence>
				<xsd:attribute ref="common:operation"/>
				<xsd:anyAttribute namespace="##any" processContents="lax"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PortErrorStats">
		<xsd:annotation>
			<xsd:documentation>Real-time port errors statistics.; Version : EXOS 12.0, Access : READ</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="common:ExosBase">
				<xsd:sequence>
					<xsd:element name="portList" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>
							Port number. When referring to a single port as in a <code>getRequest</code> to fetch specific port, the value
							is just that port number.
							<br/>
							This is also used to specify a set of ports in a <code>getRequest</code> to fetch
							multiple ports. In this case, the value is a comma separated list of ports or port ranges.
							For example, "1:1, 1:3, 2:1-2:3" ports 1:1, 1:3, 2:1,2:2,2:3.
							; Version : EXOS 12.0, Access : READ, INDEX</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="linkState" type="LinkStates" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Port link state.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="displayString" type="common:StringLen16" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>User configured name or display string of the port.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transmitCollisions" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The total number of collisions seen by the port, regardless of whether a device connected to the port participated in any of the collisions.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transmitLateCollisions" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The total number of collisions that have occurred after the ports transmit window has expired.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transmitDeferred" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The total number of frames that were transmitted by the port after the first transmission attempt was deferred by other network traffic.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transmitErrors" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The total number of frames that were not completely transmitted by the port because of network errors (such as late collisions or excessive collisions).; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transmitLost" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The total number of transmit frames that do not get completely transmitted because of buffer problems (FIFO underflow).; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="transmitParity" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The bit summation has a parity mismatch.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="receiveCrc" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The total number of frames received by the port that were of the correct length, but contained a bad FCS value. ; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="receiveOver" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The total number of good frames received by the port greater than the supported maximum length of 1,522 bytes.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="receiveUnder" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The total number of frames received by the port that were less than 64 bytes long.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="receiveFragment" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation> The total number of frames received by the port were of incorrect length and contained a bad FCS value.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="receiveJabber" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation> The total number of frames received by the port that was of greater than the support maximum length and had a Cyclic Redundancy Check (CRC) error.; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="receiveAlign" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The total number of frames received by the port that occurs if a frame has a CRC error and does not contain an integral number of octets. ; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="receiveLost" type="common:uint64_t" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The total number of frames received by the port that were lost because of buffer overflow in the switch. ; Version : EXOS 12.0, Access : READ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="extension" type="common:ExtensionType" minOccurs="0"/>
					<xsd:any namespace="##other" processContents="lax" minOccurs="0"/>
				</xsd:sequence>
				<xsd:attribute ref="common:operation"/>
				<xsd:anyAttribute namespace="##any" processContents="lax"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
